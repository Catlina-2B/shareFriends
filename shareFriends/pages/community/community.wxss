/* pages/community/community.wxss */
@import "../../compoment/dyamic/dyamic.wxss";
@import "../../compoment/square/square.wxss";
@import "../../compoment/family/family.wxss";
page{
  background-color: #e9e9e9;
}

.top{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx;
  background-color: #ffffff;
}

.top .tab{
  display: flex;
  flex-direction: row;
  font-size: 30rpx;
}

.top .tab view{
  width: 120rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 30rpx;
}

.top .tab .active{
  background-color: #f2f;
  color: #ffffff;
}

.photo{
  width: 50rpx;
  height: 50rpx;
}

.photo image{
  width: 100%;
  height: 100%;
}

.family{
  width: 50rpx;
  height: 50rpx;
}

.family image{
  width: 100%;
  height: 100%;
}